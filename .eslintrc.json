{
  "extends": ["react-app"],
  "rules": {
    "no-unused-vars": ["warn", { 
      "argsIgnorePattern": "^_",
      "varsIgnorePattern": "^_",
      "caughtErrorsIgnorePattern": "^_"
    }],
    "react-hooks/exhaustive-deps": "warn",
    "default-case": "warn"
  },
  "env": {
    "browser": true,
    "node": true
  }
}

